{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { defineComponent, ref } from \"vue\";\nimport SearchField from \"./SearchField.vue\";\nimport NodeObject from \"./NodeObject.vue\";\nconst selectedNodeId = ref(null);\nconst handleSelection = (id, isSelected) => {\n  if (isSelected) {\n    selectedNodeId.value = id;\n  } else {\n    selectedNodeId.value = null;\n  }\n};\nexport default defineComponent({\n  name: \"MainCanvas\",\n  components: {\n    SearchField,\n    NodeObject\n  },\n  setup() {\n    const nodes = ref([]);\n    const addNode = event => {\n      // if the user clicks a NodeObject, we don't want to add a new node\n      if (event.target.closest(\".node-object\")) {\n        event.stopPropagation();\n        return;\n      }\n      const rect = event.target.getBoundingClientRect();\n      const x = event.clientX - rect.left - 25;\n      const y = event.clientY - rect.top - 75;\n      nodes.value.push({\n        id: Date.now(),\n        x,\n        y\n      });\n    };\n    return {\n      nodes,\n      addNode\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","ref","SearchField","NodeObject","selectedNodeId","handleSelection","id","isSelected","value","name","components","setup","nodes","addNode","event","target","closest","stopPropagation","rect","getBoundingClientRect","x","clientX","left","y","clientY","top","push","Date","now"],"sources":["/Users/ivan/dev/knime-frontend-challenge/interactive-workflow/src/components/MainCanvas.vue"],"sourcesContent":["<template>\n  <div class=\"main-canvas\" @click=\"addNode\">\n    <SearchField />\n    <NodeObject\n      v-for=\"node in nodes\"\n      :key=\"node.id\"\n      :initialX=\"node.x\"\n      :initialY=\"node.y\"\n      @selected=\"handleSelection(node.id, $event)\"\n    />\n  </div>\n</template>\n\n<script>\nimport { defineComponent, ref } from \"vue\";\nimport SearchField from \"./SearchField.vue\";\nimport NodeObject from \"./NodeObject.vue\";\n\nconst selectedNodeId = ref(null);\n\nconst handleSelection = (id, isSelected) => {\n  if (isSelected) {\n    selectedNodeId.value = id;\n  } else {\n    selectedNodeId.value = null;\n  }\n};\n\nexport default defineComponent({\n  name: \"MainCanvas\",\n  components: {\n    SearchField,\n    NodeObject,\n  },\n  setup() {\n    const nodes = ref([]);\n\n    const addNode = (event) => {\n      // if the user clicks a NodeObject, we don't want to add a new node\n      if (event.target.closest(\".node-object\")) {\n        event.stopPropagation();\n        return;\n      }\n      const rect = event.target.getBoundingClientRect();\n      const x = event.clientX - rect.left - 25;\n      const y = event.clientY - rect.top - 75;\n      nodes.value.push({ id: Date.now(), x, y });\n    };\n\n    return { nodes, addNode };\n  },\n});\n</script>\n\n<style scoped>\n.main-canvas {\n  border: 1px solid black;\n  margin-left: 50px;\n  margin-right: 50px;\n  margin-bottom: 50px;\n  height: calc(100vh - 150px);\n  width: calc(100vw - 100px);\n  position: relative;\n}\n</style>\n"],"mappings":";AAcA,SAASA,eAAe,EAAEC,GAAE,QAAS,KAAK;AAC1C,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,OAAOC,UAAS,MAAO,kBAAkB;AAEzC,MAAMC,cAAa,GAAIH,GAAG,CAAC,IAAI,CAAC;AAEhC,MAAMI,eAAc,GAAIA,CAACC,EAAE,EAAEC,UAAU,KAAK;EAC1C,IAAIA,UAAU,EAAE;IACdH,cAAc,CAACI,KAAI,GAAIF,EAAE;EAC3B,OAAO;IACLF,cAAc,CAACI,KAAI,GAAI,IAAI;EAC7B;AACF,CAAC;AAED,eAAeR,eAAe,CAAC;EAC7BS,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVR,WAAW;IACXC;EACF,CAAC;EACDQ,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIX,GAAG,CAAC,EAAE,CAAC;IAErB,MAAMY,OAAM,GAAKC,KAAK,IAAK;MACzB;MACA,IAAIA,KAAK,CAACC,MAAM,CAACC,OAAO,CAAC,cAAc,CAAC,EAAE;QACxCF,KAAK,CAACG,eAAe,EAAE;QACvB;MACF;MACA,MAAMC,IAAG,GAAIJ,KAAK,CAACC,MAAM,CAACI,qBAAqB,EAAE;MACjD,MAAMC,CAAA,GAAIN,KAAK,CAACO,OAAM,GAAIH,IAAI,CAACI,IAAG,GAAI,EAAE;MACxC,MAAMC,CAAA,GAAIT,KAAK,CAACU,OAAM,GAAIN,IAAI,CAACO,GAAE,GAAI,EAAE;MACvCb,KAAK,CAACJ,KAAK,CAACkB,IAAI,CAAC;QAAEpB,EAAE,EAAEqB,IAAI,CAACC,GAAG,EAAE;QAAER,CAAC;QAAEG;MAAE,CAAC,CAAC;IAC5C,CAAC;IAED,OAAO;MAAEX